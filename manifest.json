{
    "description": "Adds hints for links, inputs, buttons, etc.",
    "manifest_version": 2,
    "name": "Hinter",
    "version": "1.0",
    "homepage_url": "https://github.com/donniebreve/hinter",
    "icons": {
        "48": "icons/border-48.png"
    },
    "permissions": ["<all_urls>"],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "run_at": "document_end",
        "js": ["hinter.js"],
        "css": ["hinter.css"]
    }]
}